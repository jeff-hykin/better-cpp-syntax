{
    "name": "better-cuda-syntax",
    "displayName": "Better CUDA Syntax",
    "description": "The bleeding edge of CUDA syntax",
    "version": "1.15.6",
    "icon": "icon.png",
    "scripts": {
        "build": "ruby scripts/generate.rb",
        "perfall": "ruby scripts/perf_all.rb",
        "prelint": "npm run build",
        "lint": "ruby scripts/lint.rb",
        "pretest": "npm run build",
        "test": "node test/source/commands/test.js",
        "genall": "npm run generate-specs -- --generate-all",
        "preversion": "npm test",
        "version": "npm run build && git add -A syntaxes",
        "pregenerate-specs": "npm run build",
        "generate-specs": "node test/source/commands/generate-specs.js",
        "gen": "npm run generate-specs -- ",
        "try": "npm run test -- --show-failure-only",
        "sort-specs": "node test/source/commands/sort-specs.js",
        "prereport": "npm run build",
        "report": "node test/source/commands/report",
        "perf": "npm run report -- perf",
        "cov": "npm run report -- coverage",
        "publish": "ruby scripts/publish.rb"
    },
    "keywords": [
        "C++",
        "cpp",
        "c",
        "objective-c",
        "objective-cpp",
        "objective-c++",
        "syntax",
        "textmate",
        "highlighting",
        "coloring",
        "color",
        "cuda"
    ],
    "engines": {
        "vscode": "^1.0.0"
    },
    "author": {
        "name": "Jeff Hykin"
    },
    "publisher": "jeff-hykin",
    "contributes": {
        "languages": [
            {
                "id": "cpp_cuda",
                "extensions": [
                    "cc",
                    "cpp",
                    "cp",
                    "cxx",
                    "c++",
                    "C",
                    "h",
                    "hh",
                    "hpp",
                    "h++"
                ],
                "configuration": "./source/languages/cpp_cuda/language_configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "cpp_cuda",
                "scopeName": "source.cpp.embedded.macro",
                "path": "./syntaxes/cpp_cuda.embedded.macro.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.embedded.assembly.cpp": "asm"
                }
            },
            {
                "language": "cpp_cuda",
                "scopeName": "source.cpp",
                "path": "./syntaxes/cpp_cuda.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.embedded.assembly.cpp": "asm",
                    "meta.preprocessor.macro.contents": "cpp_cuda"
                }
            }
        ]
    }
}